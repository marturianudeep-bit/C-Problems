#include<stdio.h>
int main(){
    int initialCash,N;
    int withdrawal;
    int riskCount;

    scanf("%d",&initialCash);
    scanf("%d",&N);

    int balance = initialCash;

    for(int i = 0; i < N; i++){
        scanf("%d",&withdrawal);
        balance -= withdrawal;

        if(balance < 5000){
            riskCount++;
        }
    }
    printf("Remaining Cash: %d\n",balance);
    printf("Risk Count: %d\n",riskCount);

    return 0;
}
