#include<stdio.h>
int main(){
    int N;
    int noise;
    int maxNoise = 0;
    int noisyCount = 0;

    scanf("%d",&N);

    for(int  i= 0; i < N; i++){
        scanf("%d",&noise);

        if( i == 0 || noise > maxNoise){
            maxNoise = noise;
        }
        if(noise > 70){
            noisyCount++;
        }
    }
    printf("Maximum Noise: %d\n",maxNoise);
    printf("Noisy Periods: %d\n",noisyCount);

    return 0;
}
