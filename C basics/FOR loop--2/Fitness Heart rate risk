#include<stdio.h>
int main(){
    int N;
    int rate;
    int maxRate = 0;
    int danger = 0;

    scanf("%d",&N);

    for(int i = 0; i < N; i++){
        scanf("%d",&rate);

        if(i == 0 || rate > maxRate){
            maxRate = rate;
        }
        if(rate > 140){
            danger++;
        }
    }
    printf("Max Heart Rate: %d\n",maxRate);
    printf("Danger Readings: %d\n",danger);

    return 0;
}
