#include<stdio.h>
int main(){
    int N,score;
    int totalRisk = 0;
    int highRisk = 0;

    scanf("%d",&N);

    for(int i = 0; i < N; i++){
        scanf("%d",&score);
        totalRisk += score;

        if(score > 50){
            highRisk++;
        }
    }
    printf("Total Risk: %d\n",totalRisk);
    printf("High Risk Sessions: %d\n",highRisk);

    return 0;
}
