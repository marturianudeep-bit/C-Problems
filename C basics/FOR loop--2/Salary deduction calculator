#include<stdio.h>
int main(){
    int salary,absentDays;
    int finalSalary;

    scanf("%d",&salary);
    scanf("%d",&absentDays);
     finalSalary = salary;

     for(int i = 0; i < absentDays; i++){
        finalSalary -= 100;
     }
     printf("Final Salary: â‚¹%d\n",finalSalary);

     return 0;
}
