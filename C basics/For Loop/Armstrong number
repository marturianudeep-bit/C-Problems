#include<stdio.h>
int main(){
    int n,temp,sum = 0,
    digits = 0;
    scanf("%d",&n);
    temp = n;
    for(int t = n; t > 0; t /= 10)
      digits++;
      for(; temp > 0; temp /= 10){
        int d = temp % 10;
        int power = 1;
        for(int i = 1; i <= digits; i++)
        power *= d;
        sum += power;
      }
      if(sum == n)
       printf("YEs");
       else
         printf("NO");
         return 0;
}
