#include<stdio.h>
int main(){
    int planType;
    float dataused;

    scanf("%d",&planType);
    scanf("%f",&dataused);

    switch (planType){
        if(dataused <= 1.0)
          printf("Normal Speed");
        else
           printf("Speed Reduced");
           break;

        case 2:
          if(dataused <= 2.0)
            printf("Normal Speed");
            else 
              printf("Extra charges applied");
              break;

              default:
              if(dataused > 2.0)
               printf("Extra charges applied");
               else
                printf("Normal speed");

    }
    return 0;
}
