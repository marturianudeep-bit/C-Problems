#include<stdio.h>
int main(){
    int category,fee;

    scanf("%d",&category);

    switch(category){
        case 1: //General
         fee = 1200;
         break;
         case 2: //OBC
          fee = 800;
          break;
          case 3: //SC/ST
           fee = 500;
           break;

           default:
            printf("Invalid Category\n");
            return 0;
    }
    printf("Exam fee â‚¹%d\n",fee);
    return 0;
}
