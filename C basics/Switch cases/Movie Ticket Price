#include<stdio.h>
int main(){
    int seatType,showTime;
    int price = 0;

    scanf("%d %d",&seatType,&showTime);

    switch(seatType){
        case 1:
           price = 150;
            break;

            case 2:
            price = 250;
            break;

            default:
             price = 0;

    }
     if(showTime >= 18){
        price += 50;
     }
     printf("Ticket Price â‚¹%d",price);
     return 0;
}
