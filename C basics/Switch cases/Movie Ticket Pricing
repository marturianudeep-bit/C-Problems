#include<stdio.h>
int main(){
    int seatType,showTime,price;

    scanf("%d",&seatType);
    scanf("%d",&showTime);

    switch(seatType){
        case 1:
         price = 150;
         break;
         case 2:
           price = 250;
           break;

           default:
            price = 150;
    }
     if(showTime >= 18)
      price += 50;

    printf("Ticket Price â‚¹%d\n",price);
    return 0;
}
