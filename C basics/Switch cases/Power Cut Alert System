#include<stdio.h>
int main(){
    int hours;

    scanf("%d",&hours);

    switch(hours <= 1){
        case 1:
          printf("Short Interruption");
           break;

           case 0:
             switch(hours <= 4){
                case 1:
                 printf("Medium Interruption");
                 break;
                 case 0:
                  printf("Long Interruption");
                  break;
             }
             break;
    }
    return 0;
}
