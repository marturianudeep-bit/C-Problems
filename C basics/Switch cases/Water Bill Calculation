#include<stdio.h>
int main(){
    int type,units;
    int bill =0;

    scanf("%d",&type);
    scanf("%d",&units);

    switch(type){
        case 1:
          if(units <= 30)
           bill = units * 5;
           else
             bill = (30 * 5) + ((units - 30) * 8);
             break;

             case 2:
             bill = units * 10;
             break;

             default:
              printf("Invalid Conection type\n");

              return 0;
    }
    printf("Bill â‚¹%d\n",bill);
    return 0;
}
