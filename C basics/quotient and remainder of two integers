#include<stdio.h>
 int main(){
    int a,b,quotient,remainder;

     a=9;
     b=2;
     quotient = a/b;
     remainder = a%b;
     printf("quotient=%d\n remainder=%d\n",quotient,remainder);

     a=10;
     b=3;
     quotient=a/b;
     remainder=a%b;
     printf("quotient=%d\n remainder=%d\n",quotient,remainder);

     a=20;
     b=4;
     quotient=a/b;
     remainder=a%b;
     printf("quotient=%d\n remainder=%d\n",quotient,remainder);

     return 0;
 }
