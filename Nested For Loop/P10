#include<stdio.h>
int main(){
    int i,j;

    for(int i = 1;i <= 5; i++){
        for(j = 1; j <= i-1; j++){
            printf("*");
        }
        printf("\n");
    }
    for(int i = 5; i >= 1; i--){
        for(j = 1; j < i + 1; j++){
            printf("*");
        }
        printf("\n");
    }
    return 0;
}
