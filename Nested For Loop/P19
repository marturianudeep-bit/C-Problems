#include<stdio.h>
int main(){
    int i,j;
    int n = 5;

    for(i = 1; i <= 2*n; i++)
    printf("* ");
    printf("\n");

    for(i = n; i >= 1; i--){
        for(j = 1; j <= i; j++)
        printf("* ");

        for(j = 1; j <= 2*(n - i); j++)
        printf(" ");

        for(j = 1; j <= i; j++)
        printf("* ");
        printf("\n");
    }
    for(i = 1; i <= n; i++){
        for(j = 1; j <= i; j++)
        printf("* ");

        for(j = 1; j <= 2*(n-i); j++)
        printf(" ");

        for(j = 1; j <= i; j++)
        printf("* ");
        printf("\n");
    }
    for(i = 1; i <= 2*n; i++)
    printf("* ");

    return 0;
}
