#include<stdio.h>
int main(){
    int N;
    int hours;
    int i = 0;
    int inactiveWeeks = 0;

    scanf("%d",&N);

    while(i < N){
        scanf("%d",&hours);

        if(hours == 0){
            inactiveWeeks++;
        }
        i++;
    }
    printf("Inactive Weeks: %d\n",inactiveWeeks);

    if(inactiveWeeks >= 3){
        printf("Risk Status: high\n");
    }else{
        printf("Risk Ststus: low\n");
    }
    return 0;
}
