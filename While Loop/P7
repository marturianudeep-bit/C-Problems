#include<stdio.h>
int main(){
    int capacity;
    int N;

    scanf("%d",&capacity);
    scanf("%d",&N);

    int i = 0;
    int change;
    int occupied = 0;
    int criticalCount = 0;

    int thresold = (capacity * 90) / 100;

    while(i < N){
        scanf("%d",&change);
        
        occupied += change;

        if(occupied > thresold){
            criticalCount++;
        }
        i++;
    }
    printf("Final Occupied Beds: %d\n",occupied);
    printf("Critical Hours: %d\n",criticalCount);

    return 0;
}
